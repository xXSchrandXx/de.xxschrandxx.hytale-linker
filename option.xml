<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="https://www.woltlab.com"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/5.5/option.xsd">
	<import>
		<categories>
			<category name="hytale_linker">
				<parent>hytale</parent>
			</category>
			<category name="hytale_linker_general">
				<parent>hytale_linker</parent>
			</category>
		</categories>
		<options>
			<option name="hytale_linker_enabled">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>hytale_linker_identity,hytale_max_uuids,hytale_name_enabled,hytale_enable_active_user,hytale_password_page</enableoptions>
			</option>
			<option name="hytale_linker_identity">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>HytaleConnectionMultiSelect</optiontype>
			</option>
			<option name="hytale_max_uuids">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<suffix>mc_uuids</suffix>
			</option>
			<option name="hytale_enable_active_user">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="hytale_enable_disable_user">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
			<option name="hytale_linker_branding">
				<categoryname>hytale_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<hidden>1</hidden>
			</option>
		</options>
	</import>
</data>
